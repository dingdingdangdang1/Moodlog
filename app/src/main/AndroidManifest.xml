<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.moodlog">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/smile"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/smile"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogshowActivity" android:screenOrientation="portrait"/>
        <activity android:name=".LogaddActivity" android:screenOrientation="portrait"/>
        <activity android:name=".LogeditActivity" android:screenOrientation="portrait"/>
        <activity android:name=".FoYuActivity" android:screenOrientation="portrait"/>
        <activity android:name=".GrowthActivity" android:screenOrientation="portrait"/>
        <activity android:name=".LifeActivity" android:screenOrientation="portrait"/>
        <activity android:name=".UpdateInfoActivity" android:screenOrientation="portrait"/>
        <activity android:name=".YaoMoodActivity" android:screenOrientation="portrait"></activity>
    </application>

</manifest>